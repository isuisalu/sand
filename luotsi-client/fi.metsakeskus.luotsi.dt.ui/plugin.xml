   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="fi.metsakeskus.luotsi.dt.ui.application"
            name="Luotsi product">
         <property
               name="appName"
               value="Luotsi product">
         </property>
         <property
               name="applicationXMI"
               value="fi.metsakeskus.luotsi.dt.ui/Application.e4xmi">
         </property>
         <property
               name="applicationCSS"
               value="platform:/plugin/fi.metsakeskus.luotsi.dt.ui/css/default.css">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="aboutImage"
               value="./icons/about.png">
         </property>
         <property
               name="aboutText"
               value="%verboseAboutText">
         </property>
         <property
               name="windowImages"
               value="./icons/icon16.gif,./icons/icon32.gif,./icons/icon48.gif,./icons/icon64.gif,./icons/icon128.gif">
         </property>
         <property
               name="startupProgressRect"
               value="5,275,445,15">
         </property>
      </product>
   </extension>
